{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Application": {
    "Name": "Identity Service Client"
  },
  "Endpoints": {
    "Domain": "https://yourdomain.com"
  },
  "Cookies": {
    "CookieExpirationHours": "3",
    "JwtCookieName": "jwtToken",
    "RefreshTokenCookieName": "refreshToken"
  },
  "JWT": {
    "Issuer": "https://identity.service",
    "Audience": "audience",
    "RefreshTokenEncryptionPassPhrase": "RefreshTokenPassPhrase",
    "PublicKeyXmlString": "PUBLIC_KEY_XML"
  },
  "IdentityService": {
    "ApiUrl": "https://identity.domain.com",
    "ApiKey": "X-API-KEY-1",
    "ForgotPasswordUrl": "https://identity.domain.com/password/forgot"
  }
}
